{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "plugin:prettier/recommended",
        "eslint:recommended",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "vue"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-empty": "off",
        "no-prototype-builtins": "off"

    }
}
